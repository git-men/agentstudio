{
  "name": "agentstudio-backend",
  "version": "0.2.0",
  "description": "Backend API for AgentStudio - AI-powered presentation editor with Claude integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agentstudio": "dist/bin/agentstudio.js"
  },
  "scripts": {
    "dev": "node --import tsx/esm --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "migrate-projects": "node scripts/migrate-projects.js"
  },
  "dependencies": {
    "agentstudio-shared": "^0.1.0",
    "@ai-sdk/anthropic": "^1.0.5",
    "commander": "^11.1.0",
    "@ai-sdk/openai": "^1.0.7",
    "@anthropic-ai/claude-code": "^2.0.10",
    "ai": "^5.0.22",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.1.6"
  },
  "author": "jeffkit <bbmyth@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/git-men/agentstudio.git"
  },
  "homepage": "https://github.com/git-men/agentstudio#readme",
  "bugs": {
    "url": "https://github.com/git-men/agentstudio/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "studio",
    "editor",
    "presentation",
    "claude",
    "react",
    "typescript",
    "api"
  ],
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
