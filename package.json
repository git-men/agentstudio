{
  "name": "agentstudio",
  "version": "0.1.0",
  "description": "Claude-powered AgentStudio with React frontend and Node.js backend, built on top of Claude Code SDK",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm start",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "ai",
    "agent",
    "studio",
    "editor",
    "react",
    "typescript"
  ],
  "author": "jeffkit <bbmyth@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffkit/agentstudio.git"
  },
  "homepage": "https://github.com/jeffkit/agentstudio#readme",
  "bugs": {
    "url": "https://github.com/jeffkit/agentstudio/issues"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eventsource": "^4.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.103",
    "diff": "^8.0.2",
    "node-fetch": "^3.3.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
