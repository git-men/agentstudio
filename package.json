{
  "name": "agentstudio",
  "version": "0.1.19",
  "description": "Claude-powered AgentStudio with React frontend and Node.js backend, built on top of Claude Code SDK",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "dev": "concurrently \"pnpm --filter @agentstudio/frontend run dev\" \"pnpm --filter agentstudio-backend run dev\"",
    "dev:frontend": "pnpm --filter frontend run dev",
    "dev:backend": "pnpm --filter backend run dev",
    "build": "pnpm --filter shared run build && pnpm --filter frontend run build && pnpm --filter backend run build",
    "build:frontend": "pnpm --filter frontend run build",
    "build:backend": "pnpm --filter shared run build && pnpm --filter backend run build",
    "start": "pnpm --filter backend run start",
    "setup": "pnpm install",
    "test": "pnpm --recursive run test --if-present",
    "lint": "pnpm --recursive run lint --if-present",
    "type-check": "pnpm --recursive run type-check --if-present",
    "clean": "pnpm --recursive run clean --if-present",
    "install:service": "sudo chmod +x install.sh && sudo ./install.sh",
    "service": "./scripts/agent-studio-service",
    "service:start": "./scripts/agent-studio-service start",
    "service:stop": "./scripts/agent-studio-service stop",
    "service:status": "./scripts/agent-studio-service status",
    "service:logs": "./scripts/agent-studio-service logs",
    "service:restart": "./scripts/agent-studio-service restart"
  },
  "keywords": [
    "ai",
    "agent",
    "studio",
    "editor",
    "react",
    "typescript"
  ],
  "author": "okguitar <okguitar@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/okguitar/agentstudio.git"
  },
  "homepage": "https://github.com/okguitar/agentstudio#readme",
  "bugs": {
    "url": "https://github.com/okguitar/agentstudio/issues"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.9.2"
  }
}
