{
  "name": "agentstudio-shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for AgentStudio",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types/agents": {
      "types": "./dist/types/agents.d.ts",
      "default": "./dist/types/agents.js"
    },
    "./types/commands": {
      "types": "./dist/types/commands.d.ts",
      "default": "./dist/types/commands.js"
    },
    "./types/subagents": {
      "types": "./dist/types/subagents.d.ts",
      "default": "./dist/types/subagents.js"
    },
    "./types/projects": {
      "types": "./dist/types/projects.d.ts",
      "default": "./dist/types/projects.js"
    },
    "./types/claude-versions": {
      "types": "./dist/types/claude-versions.d.ts",
      "default": "./dist/types/claude-versions.js"
    },
    "./types/claude-history": {
      "types": "./dist/types/claude-history.d.ts",
      "default": "./dist/types/claude-history.js"
    },
    "./utils/agentStorage": {
      "types": "./dist/utils/agentStorage.d.ts",
      "default": "./dist/utils/agentStorage.js"
    },
    "./utils/toolMapping": {
      "types": "./dist/utils/toolMapping.d.ts",
      "default": "./dist/utils/toolMapping.js"
    },
    "./utils/projectMetadataStorage": {
      "types": "./dist/utils/projectMetadataStorage.d.ts",
      "default": "./dist/utils/projectMetadataStorage.js"
    },
    "./utils/claudeVersionStorage": {
      "types": "./dist/utils/claudeVersionStorage.d.ts",
      "default": "./dist/utils/claudeVersionStorage.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^20.5.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.1"
  },
  "author": "okguitar <okguitar@gmail.com>",
  "license": "MIT",
  "private": true
}